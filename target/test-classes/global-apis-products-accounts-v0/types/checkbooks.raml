#%RAML 1.0 Library
uses:
  annotations: ../../global-apis-commons-commons-v0/annotations/annotationsLibrary.raml
  checkbookType: checkbookType.raml
  checkbookFormat: checkbookFormat.raml
  checkbookStatus: checkbookStatus.raml

types:
  checkbooks:
    type: array
    items:
      properties:
        checkbookId:
          (annotations.bindingDefinition):
            - method: "GET"
              input: "NONE"
              output: "REQUIRED"
          type: string
          description: |
            Checkbook identifier.
          required: false
        checkbookType:
          (annotations.bindingDefinition):
            - method: "GET"
              input: "NONE"
              output: "REQUIRED"
          type: checkbookType.checkbookType
          description: |
            Checkbook type.
          required: false
        format:
          (annotations.bindingDefinition):
            - method: "GET"
              input: "NONE"
              output: "REQUIRED"
          type: checkbookFormat.checkbookFormat
          description: |
            Printed format.
          required: false
        requestDate:
          (annotations.bindingDefinition):
            - method: "GET"
              input: "NONE"
              output: "REQUIRED"
          type: date-only
          description: |
            String based on ISO-8601 date format for specifying when the user requested the checkbook.
          required: false
        deliveryDate:
          (annotations.bindingDefinition):
            - method: "GET"
              input: "NONE"
              output: "REQUIRED"
          type: date-only
          description: |
            String based on ISO-8601 date format for specifying when the user picked up the checkbook.
          required: false
        status:
          (annotations.bindingDefinition):
            - method: "GET"
              input: "NONE"
              output: "REQUIRED"
          type: checkbookStatus.checkbookStatus
          description: |
            Checkbook status.
          required: false
        firstCheckId:
          (annotations.bindingDefinition):
            - method: "GET"
              input: "NONE"
              output: "REQUIRED"
          type: string
          description: |
            Numeration of the checkbook\'s first check.
          required: false
        lastCheckId:
          (annotations.bindingDefinition):
            - method: "GET"
              input: "NONE"
              output: "REQUIRED"
          type: string
          description: |
            Numeration of the checkbook\'s last check.
          required: false
