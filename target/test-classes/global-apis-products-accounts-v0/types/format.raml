#%RAML 1.0 Library
uses:
  annotations:  ../../global-apis-commons-commons-v0/annotations/annotationsLibrary.raml
  accountNumberFormat: accountNumberFormat.raml

types:
  format:
    type: object
    properties:
        number:
          (annotations.bindingDefinition):
            - method: "GET"
              input: "NONE"
              output: "REQUIRED"
            - method: "POST"
              input: "OPTIONAL"
              output: "OPTIONAL"
          type: string
          required: false
          description: |
            Account number.
        numberType:
          (annotations.bindingDefinition):
            - method: "GET"
              input: "NONE"
              output: "REQUIRED"
            - method: "POST"
              input: "OPTIONAL"
              output: "OPTIONAL"
          type: accountNumberFormat.accountNumberFormat
          required: false
          description: |
            Type of account number.
